{
  "compilerOptions": {
      "target": "es6",            
      "module": "commonjs",       
      "sourceMap": true,         
      "outDir": "./build",        
      "rootDir": "./src",        
      "removeComments": true,    
      "noImplicitAny": false,       
      "noEmitOnError": true,
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
  }
}